@model List<Tickets.tickets>

<table class="table">
    <thead>
        <tr>
            <th>
                nombre
            </th>
            <th>
                descripcion
            </th>
            <th>
                departamento
            </th>
            <th></th>
        </tr>
    </thead>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.nombre
            </td>
            <td>
                @item.descripcion
            </td>
            <td>
                @item.departamento
            </td>
        </tr>
    }

</table>
